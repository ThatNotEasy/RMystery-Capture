{
  "background": {
    "persistent": true,
    "scripts": ["background.js"]
  },
  "manifest_version": 2,
  "name": "RMystery Capture 2.0",
  "browser_action": { "default_icon": "icons/icon.png" },
  "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "*://*/*",
    "tabs",
    "clipboardWrite",
    "storage"
  ],
  "version": "1.0",
  "icons": {
    "48": "icons/icon.png",
    "96": "icons/icon.png"
  }
}
